# Copy this file to .env.local and fill in your values

# Database - For local development, use a PostgreSQL database
DATABASE_URL="postgresql://user:password@localhost:5432/stock_predictions?schema=public"

# NextAuth Configuration
NEXTAUTH_SECRET="generate-a-random-secret-key-here"
NEXTAUTH_URL="http://localhost:3000"

# Cron Job Secret - Generate a random string for security
CRON_SECRET="generate-a-random-cron-secret-here"

# Optional: Stock Data API (for fetching real-time prices)
ALPHA_VANTAGE_API_KEY="your-api-key-here"
